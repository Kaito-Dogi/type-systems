{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "skipLibCheck": true,
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "src/*": ["./src/*"]
    },
    "plugins": [
      {
        "name": "eslint-plugin-import-access"
      }
    ]
  },
  "includes": ["src/**/*.ts", "src/**/*.tsx"],
  "exclude": ["node_modules"]
}
